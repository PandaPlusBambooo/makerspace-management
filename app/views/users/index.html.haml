-content_for :css do
  %link{href: "https://cdn.datatables.net/v/dt/jszip-2.5.0/dt-1.10.18/b-1.5.4/b-colvis-1.5.4/b-flash-1.5.4/b-html5-1.5.4/fh-3.1.4/r-2.2.2/sl-1.2.6/datatables.min.css", rel: "stylesheet", type: "text/css"}/



.jumbotron
  %h2
    All Users
    %title
  = content_tag(:table, role: :datatable, data: { url: users_path(format: :json)},:style => 'width:100%', :class => 'stripe hover row-border') do
    %thead
      %tr
        %th First Name
        %th Last Name
        %th Email
        %th User Type
        %th ID
        %th Member Since
        %th Sign-in Count
        %th
    %tbody


  - content_for :javascript do
    %script{src: "https://cdn.datatables.net/v/dt/jszip-2.5.0/dt-1.10.18/b-1.5.4/b-colvis-1.5.4/b-flash-1.5.4/b-html5-1.5.4/fh-3.1.4/r-2.2.2/sl-1.2.6/datatables.min.js", type: "text/javascript"}
    %script{src: "https://cdn.datatables.net/tabletools/2.2.4/js/dataTables.tableTools.min.js", type: "text/javascript"}
    %script{src: "https://cdn.datatables.net/tabletools/2.2.2/swf/copy_csv_xls_pdf.swf", type: "text/javascript"}
    %script{src: "https://cdn.datatables.net/1.10.11/js/jquery.dataTables.min.js", type: "text/javascript"}
    %script{src: "https://cdn.datatables.net/buttons/1.1.2/js/dataTables.buttons.min.js", type: "text/javascript"}
    %script{src: "https://cdn.datatables.net/buttons/1.1.2/js/buttons.flash.min.js", type: "text/javascript"}
    %script{src: "https://cdnjs.cloudflare.com/ajax/libs/jszip/2.5.0/jszip.min.js", type: "text/javascript"}
    %script{src: "https://cdn.rawgit.com/bpampuch/pdfmake/0.1.18/build/pdfmake.min.js", type: "text/javascript"}
    %script{src: "https://cdn.rawgit.com/bpampuch/pdfmake/0.1.18/build/vfs_fonts.js", type: "text/javascript"}
    %script{src: "https://cdn.datatables.net/buttons/1.1.2/js/buttons.html5.min.js", type: "text/javascript"}
